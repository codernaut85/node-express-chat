{
	"name": "express-chat",
	"version": "1.0.0",
	"description": "A simple NodeJS and Express chat app",
	"main": "index.js",
	"scripts": {
		"clean": "rimraf dist/*",
		"client": "rimraf dist/client.js && webpack --config webpack.clientConfig.js",
		"server": "rimraf dist/server.js && webpack --config webpack.serverConfig.js",
		"build": "rimraf dist/server.js && webpack --config webpack.serverConfig.js && webpack --config webpack.clientConfig.js",
		"start": "node ./dist/server.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://codernaut85@github.com/codernaut85/node-express-chat.git"
	},
	"keywords": [
		"node",
		"express",
		"chat"
	],
	"author": "Grant Sykes",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/codernaut85/node-express-chat/issues"
	},
	"homepage": "https://github.com/codernaut85/node-express-chat#readme",
	"devDependencies": {
